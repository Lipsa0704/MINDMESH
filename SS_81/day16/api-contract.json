{
  "title": "User API Contract",
  "baseUrl": "https://api.example.com",

  "endpoints": [
    {
      "name": "Get Users",
      "method": "GET",
      "path": "/users",
      "response": {
        "status": 200,
        "body": [
          {
            "id": 1,
            "name": "Suchismita",
            "email": "suchi@example.com"
          }
        ]
      }
    },

    {
      "name": "Create User",
      "method": "POST",
      "path": "/users",
      "request": {
        "name": "string",
        "email": "string"
      },
      "response": {
        "status": 201,
        "message": "User created successfully"
      }
    }
  ]
}
